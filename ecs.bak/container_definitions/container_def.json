[
  {
    "name": "nginx",
    "image": "032796414879.dkr.ecr.us-east-1.amazonaws.com/scubadivedubai-api-nginx",
    "memory": 256,
    "cpu": 256,
    "essential": true,
    "portMappings": [
      {
        "containerPort": 443,
        "protocol": "tcp"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "awslogs-nginx-ecs",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "nginx"
      }
    }
  },
  {
    "name": "${container_name}",
    "image": "${image}",
    "cpu": 256,
    "memory": 512,
    "essential": true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${log_group}",
        "awslogs-region": "${region}",
        "awslogs-stream-prefix": "${container_name}"
      }
    }
  }
]
